{
  "name": "fish-language-support",
  "displayName": "Itasca Fish Language Support",
  "version": "0.0.1",
  "publisher": "Pouria Taleghani",
  "repository": {
    "type": "git",
    "url": "https://github.com/prtlq/Fish"
  },

  "description": "Syntax highlighting and support for the Itasca FISH language used in numerical modeling software like FLAC3D, PFC, and 3DEC.",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "fish",
        "aliases": ["FISH", "fish"],
        "extensions": [".fis"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "fish",
        "scopeName": "source.fish",
        "path": "./syntaxes/fish.tmLanguage.json"
      }
    ],
    "themes": [
      {
        "label": "Fish Default Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/fish-color-theme.json"
      }
    ]
  }
}
